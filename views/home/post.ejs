<!doctype html>
<html lang="en" ng-app='fwrk.home' ng-controller="PostCtrl">
    <head>
        <meta charset="utf-8">
        <title><%- metaTagsTitle %></title>
        <meta content="<%- metaDescription %>" name="description" />
        <meta content="<%- metaTagsKeyWords %>" name="keywords" />
        <meta property="og:title" content="<%- metaTagsTitle %>" />
        <meta property="og:url"   content="<%- metaTagsUrl %>" />
        <meta property="og:description" content="<%- metaDescription %>" />
        <!-- Schema.org markup for Google+ -->
        <meta itemprop="description" content="<%- metaDescription %>" />
        <!-- Twitter Card data -->    
        <meta name="twitter:site" content="<%- metaTagsUrl %>">
        <meta name="twitter:url" content="<%- metaTagsUrl %>">
        <meta name="twitter:title" content="<%- metaTagsTitle %>">
        <meta name="twitter:description" content="<%- metaDescription %>">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"  crossorigin="anonymous">
        <!-- Optional theme -->      
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" >
        <link rel="stylesheet" type="text/css" href="/home/scss/scss/home.css">
        <!-- Fonts Stylesheet Include Here -->
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" />
        <link rel="stylesheet" type="text/css" href="/home/scss/scss/fonts.css">
        <!--[if lt IE 9]>
        <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    </head>
    <body>
        <!-- Main Content -->
        <div ng-include='"/home/templates/element/blog_header.html"'></div>
        <div ng-include='"/home/templates/blog/post.html"'></div>
        <div ng-include='"/home/templates/element/footer.html"'></div>
        <!-- Angular Material Dependencies -->
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
        <!-- UI Router -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.15/angular-ui-router.js"></script>
        <!-- Angular Material -->
        <script src="https://ajax.googleapis.com/ajax/libs/angular_material/0.10.0/angular-material.min.js"></script>
        <!-- Custom JS -->
        <script src='/home/js/app_page.js'></script>
        <script src='/home/js/controllers/post.js'></script>
        <script src='/common/modules/posts.js'></script> 
        <script src='/common/modules/pages.js'></script>  
        <script src='/common/modules/categories.js'></script>
        <script>
//           var getTest = "<%= metaTagsKeyWords  %>"; 
//           console.log(getTest); 
        </script>
    </body>
</html>